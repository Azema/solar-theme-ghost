{{!< default}}


{{#foreach posts}}

    <article class="{{post_class}}">
        <h2>
        	<span class="meta"><time datetime="{{date format='YYYY-MM-DD'}}">{{date format="DD MMM YYYY"}}</time></span>
        	<div class="title">
    	    	<a href="{{url}}">{{title}}</a>
    	    </div>
        </h2>
        <div class="post-content"><p>{{excerpt}}&hellip;</p></div>
    </article>

{{/foreach}}

{{pagination}}
